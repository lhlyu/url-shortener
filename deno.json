{
	"lock": false,
	"tasks": {
		"start": "deno run -A --watch=static/,routes/ main.ts"
	},
	"lint": {
		"rules": {
			"tags": [
				"recommended"
			]
		}
	},
	"fmt": {
		"useTabs": true,
		"lineWidth": 80,
		"indentWidth": 4,
		"semiColons": false,
		"singleQuote": true,
		"proseWrap": "preserve"
	},
	"imports": {
		"$std/": "https://deno.land/std@0.197.0/"
	},
	"compilerOptions": {
	}
}
